package vault{
    
    /**
     * @private
     * Safe is a parent class for sub-safe classes which are used to store and obfuscate a sensitive data
     * for the Vault class.
     */
    internal class SafeParent{
        protected var unchanged:Number = 0;
        
        internal function get()                                      :Number { return NaN;   } 
        internal function set(newNum:Number, skipCheck:Boolean=false):Number { return NaN;   }
        internal function check()                                    :Boolean{ return false; }
        
        internal function safeToString()             :String{ return ""; }
        internal function stringToSafe(string:String):void  {}
    }
}